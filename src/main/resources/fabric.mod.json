{
	"schemaVersion": 1,
	"id": "aiottools",
	"version": "${version}",
	"name": "AiotTools",
	"description": "A simple mod to add AIOT tools",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/aiottools/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "net.fabric.aiottools.AiotTools",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "net.fabric.aiottools.AiotToolsDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"aiottools.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}